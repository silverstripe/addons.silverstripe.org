{
	"name": "extensions.silverstripe.org",
	"description": "The SilverStripe extensions listing site",
	"repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mandrew/silverstripe-globaltoolbar.git"
        },
        {	
        	"url": "https://github.com/chillu/silverstripe-elastica.git",
        	"type": "vcs"
        }
    ],	
	"require": {
		"composer/installers": "*",
		"ajshort/silverstripe-elastica": "dev-pulls/resultlist-missing-objs",
		"chrisboulton/php-resque": "1.2.*",
		"composer/composer": "*",
		"dflydev/markdown": "1.0.*",
		"ezyang/htmlpurifier": "4.5.*",
		"guzzle/guzzle": "3.2.*",
		"silverstripe/framework": "3.1.*",
		"silverstripe/multivaluefield": "*",
		"knplabs/packagist-api": "*",
		"stojg/silverstripe-resque": "*",
		"silverstripe/toolbar": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "3.7.*",
		"silverstripe/sqlite3": "*"
	},
	"minimum-stability": "dev"
}
